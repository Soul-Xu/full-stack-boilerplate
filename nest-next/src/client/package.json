    "@types/react-dom": "^18.2.0",
    "ant-design": "^1.0.0",
    "antd": "^4.20.2",
    "antd-img-crop": "^4.2.3",
    "array-move": "^4.0.0",
    "axios": "^0.27.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "bignumber.js": "^9.1.0",
    "classnames": "^2.3.1",
